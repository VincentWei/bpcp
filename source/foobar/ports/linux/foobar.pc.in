prefix=@prefix@
exec_prefix=@exec_prefix@
includedir=${prefix}/@includedir@
libdir=${prefix}/@libdir@

Name: FooBar
Description: The sample profject for VincentWei's C language course.
URL: https://github.com/VincentWei/best-practices-of-c
Version: @PROJECT_VERSION@
Requires.private: @FOOBAR_PKGCONFIG_REQUIRES@
Libs: -L${libdir} -lfoobar
Libs.private: @FOOBAR_PKGCONFIG_LIBS@
Cflags: -I${includedir} @FOOBAR_PKGCONFIG_CFLAGS@
